{
  "manifest_version": 3,
  "name": "Jump Scares",
  "description": "Jump Scares for Funsies Extension",
  "version": "1.0",

  "action": {
    "default_popup": "jumpscare.html",
    "default_icon": "Jumpscareimg.png"
  },

  "content_scripts":[
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["scripts/popup.js"]
    }
  ],
  "background": [
    {
      "service_worker": "background.js"
    }
  ]

}